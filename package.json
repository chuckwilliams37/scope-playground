{
  "name": "scope-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"convex dev\"",
    "build": "convex codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "parity": "ts-node --project tools/tsconfig.json tools/parity/check-parity.ts",
    "parity:check": "ts-node --project tools/tsconfig.json tools/parity/check-parity.ts --mode check --json data/stories.json",
    "parity:pull": "ts-node --project tools/tsconfig.json tools/parity/check-parity.ts --mode pull --json data/stories.json",
    "parity:push": "ts-node --project tools/tsconfig.json tools/parity/check-parity.ts --mode push --json data/stories.json",
    "simple-parity": "ts-node --project tools/tsconfig.json tools/parity/simple-parity-checker.ts",
    "test": "jest",
    "test:parity": "jest tools/parity/__tests__",
    "test:clientsafe": "jest tests/estimation tests/export --testPathPattern='(redact|csv_clientsafe)'",
    "test:watch": "jest --watch",
    "delta:dry": "ts-node --project tools/tsconfig.json tools/resolve-delta/run.ts",
    "delta:apply": "APPLY=true ts-node --project tools/tsconfig.json tools/resolve-delta/run.ts",
    "delta:revert": "ts-node --project tools/tsconfig.json tools/resolve-delta/revert.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.2",
    "framer-motion": "^12.23.21",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "next": "^15.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stripe": "^14.25.0",
    "vercel": "^48.1.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.17",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "convex": "^1.27.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.4",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "zod": "^3.22.4"
  }
}
